{{> header}}

<div class="profile col-md-6 text-center col-lg">
    <img class="profile-img" src="./images/user.png" />
    <div class="profile-info">
        <p>Email: <small>{{email}}</small></p>
        <p class="infoType">Wished {{count}} treks =)</p>
        {{#each trecks}}
        <p>{{this.location}}</p>
        {{else}}
        <p>No treks</p>
        {{/each}}
    </div>
</div>

{{> footer}}